<!DOCTYPE html>
<html>
<head>
    <title>@FrontHtml.HtmlTitle()</title>
    @FrontHtml.HeadContent()
</head>
<body>
    <div id="wrapper">
        @FrontHtml.Shared("Header")
        @FrontHtml.Shared("Navigation")
        <div id="main" class="clearfix">
            <div class="bgcontent">
                <div id="content">
                    @FrontHtml.Zone("content")
                </div>
                <div id="sidebar">
                    @FrontHtml.Zone("sidebar")
                </div>
            </div>
            <div class="bgbottom">
            </div>
        </div>
        @FrontHtml.Shared("Footer")
    </div>
</body>
</html>
