<!DOCTYPE html>
<html>
<head>
    <title>@FrontHtml.HtmlTitle()</title>
    @FrontHtml.HeadContent()
</head>
<body>
    <div id="wrapper">
        @FrontHtml.Shared("Header")
        <div id="page-introduction">
            <div class="inner clearfix">
                <h2 class="first">
                    @PageLabel("Another Open Source CMS?")</h2>
                <h2 class="right">
                    @PageLabel("Why choose it?")
                </h2>
            </div>
        </div>
        <div id="main" class="clearfix">
            <div class="inner">
                <div id="content">
                    @FrontHtml.Zone("content")
                </div>
                <div id="rsidebar">
                    @FrontHtml.Zone("rsidebar")
                </div>
            </div>
        </div>
        @FrontHtml.Shared("Footer")
    </div>
</body>
</html>
